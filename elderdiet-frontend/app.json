{
  "expo": {
    "name": "ElderDiet",
    "slug": "ElderDiet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/my-icon.png",
    "scheme": "elderdiet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/my-splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zeroh12.ElderDiet",
      "buildNumber": "1",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This app uses tracking to provide personalized content.",
        "NSCameraUsageDescription": "This app uses the camera to take photos for meal records.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to select images for meal records.",
        "NSMicrophoneUsageDescription": "This app uses the microphone for voice input features.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "8.153.204.247": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSIncludesSubdomains": false
            },
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSIncludesSubdomains": false
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/my-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.zeroh12.ElderDiet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/my-favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/my-icon.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "36ea1d9a-f68a-4445-a8fa-c22c49972703"
      }
    }
  }
}
